{
  "name": "composedb",
  "private": true,
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "docs": "typedoc --tsconfig tsconfig.docs.json",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "lint": "turbo run lint",
    "build": "pnpm --filter \"@composedb/*\" build:types && turbo run build:js",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.3",
    "@jest/globals": "^29.0.1",
    "@skypack/package-check": "^0.2.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.244",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "del-cli": "^5.0.0",
    "eslint": "^8.23.0",
    "eslint-config-3box": "^0.4.1",
    "eslint-plugin-jest": "^27.0.1",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "turbo": "^1.4.3",
    "typedoc": "0.23.11",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.8.2"
  },
  "pnpm": {
    "overrides": {
      "@ceramicnetwork/common": "link:../../ceramicnetwork/js-ceramic/packages/common",
      "@ceramicnetwork/core": "link:../../ceramicnetwork/js-ceramic/packages/core",
      "@ceramicnetwork/http-client": "link:../../ceramicnetwork/js-ceramic/packages/http-client",
      "@ceramicnetwork/stream-caip10-link-handler": "link:../../ceramicnetwork/js-ceramic/packages/stream-caip10-link-handler",
      "@ceramicnetwork/stream-model": "link:../../ceramicnetwork/js-ceramic/packages/stream-model",
      "@ceramicnetwork/stream-model-handler": "link:../../ceramicnetwork/js-ceramic/packages/stream-model-handler",
      "@ceramicnetwork/stream-model-instance-handler": "link:../../ceramicnetwork/js-ceramic/packages/stream-model-instance-handler",
      "@ceramicnetwork/stream-tile-handler": "link:../../ceramicnetwork/js-ceramic/packages/stream-tile-handler",
      "@ceramicnetwork/streamid": "link:../../ceramicnetwork/js-ceramic/packages/streamid"
    }
  }
}
