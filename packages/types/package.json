{
  "name": "@composedb/types",
  "version": "0.6.0-rc.1",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://composedb.js.org",
  "keywords": [
    "ceramic",
    "composedb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-composedb",
    "directory": "packages/types"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "pnpm build:clean && pnpm build:types && pnpm build:js",
    "lint": "eslint src --fix",
    "prepare": "pnpm build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@ceramicnetwork/common": "^3.0.4",
    "@ceramicnetwork/stream-model": "^2.0.4",
    "@ceramicnetwork/stream-model-instance": "^2.0.4",
    "dids": "^4.0.4",
    "json-schema-typed": "^8.0.1"
  }
}
