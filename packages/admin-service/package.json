{
  "name": "@composedb/admin-service",
  "version": "0.4.0",
  "author": "3Box Labs",
  "license": "(Apache-2.0 OR MIT)",
  "homepage": "https://github.com/ceramicstudio/js-composedb#readme",
  "keywords": [
    "ceramic",
    "composedb"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ceramicstudio/js-composedb",
    "directory": "packages/admin-service"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build:schema": "node ./scripts/print-schema.mjs",
    "build": "pnpm build:clean && pnpm build:types && pnpm build:js && pnpm build:schema",
    "lint": "eslint src --fix",
    "prepare": "pnpm build",
    "prepublishOnly": "package-check"
  },
  "dependencies": {
    "@composedb/services-rpc": "workspace:*",
    "@trpc/client": "^10.27.1",
    "@trpc/server": "^10.27.1",
    "dataloader": "^2.2.2",
    "fp-ts": "^2.15.0",
    "graphql": "^16.6.0",
    "graphql-relay": "^0.10.0",
    "io-ts": "^2.2.20",
    "nanoid": "^4.0.2",
    "rxjs": "^7.8.1",
    "rxjs-for-await": "^1.0.0"
  },
  "devDependencies": {
    "@composedb/database-service": "workspace:*"
  }
}
